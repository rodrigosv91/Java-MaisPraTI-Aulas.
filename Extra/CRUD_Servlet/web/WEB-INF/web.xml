<?xml version="1.0" encoding="UTF-8"?>
<web-app version="4.0" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd">
    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
    </welcome-file-list>
    
    <!--LISTA STUFF -->
    <servlet>
        <servlet-name>lista_stuf_servlet</servlet-name>
        <servlet-class>controller.listaStuffServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>lista_stuf_servlet</servlet-name>
        <url-pattern>/lista_stuff</url-pattern>
    </servlet-mapping>
    
    <!--INSERT STUFF -->
    <servlet>
        <servlet-name>insert_stuff_servlet</servlet-name>
        <servlet-class>controller.InsertStuffServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>insert_stuff_servlet</servlet-name>
        <url-pattern>/insert_stuff</url-pattern>
    </servlet-mapping>
    
    <!--DELETE STUFF -->
        <servlet>
        <servlet-name>delete_stuff_servlet</servlet-name>
        <servlet-class>controller.DeleteStuffServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>delete_stuff_servlet</servlet-name>
        <url-pattern>/delete_stuff</url-pattern>
    </servlet-mapping>
    
</web-app>
